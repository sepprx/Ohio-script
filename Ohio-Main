old = hookfunction(require(game.ReplicatedStorage["devv-framework"].client.Objects.v3item.modules.melee).meleeHitreg,function(...) 
    local args = {...}

    if args[2] == "swing" then
        if OnePunch == true then
            return old(args[1],"megaswing")
        end
    end
    
    if args[2] == "punch" or args[2] == "kick" then
        if OnePunch == true then
            return old(args[1],"megapunch")
        end
    end
    
    return old(...)
end)

