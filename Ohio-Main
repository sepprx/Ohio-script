Player_Misc:Button({
    Name = "Trigger military gate",
    Callback = function()
        local oldpos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        tpcframe(game:GetService("Workspace").Game.Props["Military Gate"]["Military Gate"].Keypad["Meshes/MilitaryGate2_Cube.009_Cube.012_Material.005"].CFrame)
        wait(0.17)
        fireproximityprompt(game:GetService("Workspace").Game.Props["Military Gate"]["Military Gate"].Keypad.ProximityPrompt)
        wait(0.17)
        tpcframe(oldpos)
    end
})


Money_AutoFarm:Toggle({
    Name = "Autofarm atm (equip fist)",
    Callback = function()
        if AutoAtmFarm == false then
            AutoAtmFarm = true
        else
            AutoAtmFarm = false
        end
    end
})


Money_AutoFarm:Toggle({
    Name = "Auto pick up money",
    Callback = function()
        if AutoPickUpMoney == false then
            AutoPickUpMoney = true
        else
            AutoPickUpMoney = false
        end
    end
})

Money_AutoFarm:Button({
    Name = "Tp to random safe",
    Callback = function()
        local found = nil

        for i,v in pairs(game:GetService("Workspace").Game.Entities.SmallSafe:GetChildren()) do
            found = v
        end

        for i,v in pairs(game:GetService("Workspace").Game.Entities.MediumSafe:GetChildren()) do
            found = v
        end

        for i,v in pairs(game:GetService("Workspace").Game.Entities.LargeSafe:GetChildren()) do
            found = v
        end

        if found ~= nil then
            tpcframe(found.Safe.CFrame + Vector3.new(0,3,0))
            game.StarterGui:SetCore("SendNotification",{
                Title = "Tp";
                Text = "Teleported u to an safe";
                Duration = 5,
                Callback = NotificationBindable
            })
        else
            game.StarterGui:SetCore("SendNotification",{
                Title = "Tp";
                Text = "No safe found";
                Duration = 5,
                Callback = NotificationBindable
            })
        end
    end
})

Money_AutoFarm:Button({
    Name = "Tp to random money printer",
    Callback = function()
        local found = nil
        for i,v in pairs(game:GetService("Workspace").Game.Local.droppables:GetChildren()) do
            if v.Name == "Money Printer" then
                found = v
            end
        end

        if found ~= nil then
            tpcframe(found.Handle.CFrame)
            game.StarterGui:SetCore("SendNotification",{
                Title = "Tp";
                Text = "Teleported u to an money printer";
                Duration = 5,
            })
        else
            game.StarterGui:SetCore("SendNotification",{
                Title = "Tp";
                Text = "No money printer found";
                Duration = 5,
            })
        end
    end
})

Money_AutoFarm:Button({
    Name = "Tp to random tool/weapon",
    Callback = function()
        local randomnumamount = #game:GetService("Workspace").Game.Entities.ItemPickup:GetChildren()
        local randomnum = math.random(1,randomnumamount)
        for i,v in pairs(game:GetService("Workspace").Game.Entities.ItemPickup:GetChildren()) do
            if i == randomnum then
                if v:IsA("Model") then
                    for i,v in pairs(v:GetChildren()) do
                        if v:IsA("Part") then
                            tpcframe(v.CFrame)
        
                            wait(0.1)
        
                            for i,v in pairs(v:GetChildren()) do
                                if v:IsA("ClickDetector") then
                                    fireclickdetector(v)
                                end
                            end
                        end
                    end
                end
            end
        end
    end
})

game:GetService("RunService").Heartbeat:Connect(function()
    if autopickuptools == true then
        for i,v in pairs(game:GetService("Workspace").Game.Entities.ItemPickup:GetChildren()) do
            if v:IsA("Model") then
                for i,v in pairs(v:GetChildren()) do
                    if v:IsA("Part") then
                        tpcframe(v.CFrame)
    
                        wait(0.05)
    
                        for i,v in pairs(v:GetChildren()) do
                            if v:IsA("ClickDetector") then
                                fireclickdetector(v)
                            end
                        end
                    end
                end
            end
        end
    end
end)

game:GetService("RunService").Heartbeat:Connect(function()
    if AutoAtmFarm == true then
        if game:GetService("Workspace").Game.Props.ATM:FindFirstChild("ATM") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Game.Props.ATM:FindFirstChild("ATM").Slot.CFrame
        
            for i,v in pairs(game:GetService("Workspace").Game.Props.ATM:GetChildren()) do
                if v:FindFirstChild("Slot") then
                    if (v.Slot.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 15 then
                        if v:GetAttribute("guid") then
                            FireRemoteEvent.FireServer("meleeHit","prop",{["meleeType"] = "punch",["guid"] = v:GetAttribute("guid")})
                        end
                    end
                end
            end
        end
    end
end)

game:GetService("RunService").Heartbeat:Connect(function()
    if AutoPickUpMoney == true then
        for i,var in pairs(game:GetService("Workspace").Game.Entities.CashBundle:GetChildren()) do
            if var:IsA("Model") then
                for i,v in pairs(var:GetChildren()) do
                    if v:IsA("Part") then
                        local oldpos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame

                        wait(0.05)

                        fireclickdetector(var.ClickDetector)

                        game.Players.LocalPlayer.Character.HumanoidRootPart.Character.HumanoidRootPart.CFrame = oldpos
                    end
                end
            end
        end
        end
end)

game.Players.LocalPlayer:GetMouse().Button1Down:Connect(function()
    if ClickToLockOrUnlockDoors == true then
        if game.Players.LocalPlayer:GetMouse().Target.Name == "Door" then
            if game.Players.LocalPlayer:GetMouse().Target.Parent.Name == "DoorMain" then
                local oldpos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                local doormainloc = game.Players.LocalPlayer:GetMouse().Target.Parent
                
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(doormainloc.Door.Position + doormainloc.Door.CFrame.LookVector * 1)
                
                wait(0.13)
                
                fireclickdetector(doormainloc.Lock.ClickDetector)
                
                wait(0.13)
                
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldpos
            end
        end
    end
end)

game:GetService("RunService").Heartbeat:Connect(function()
    if Invincible1 == true then
        if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
            game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Health = 150
        end
    end
end)

game:GetService("RunService").Heartbeat:Connect(function()
    if ChangeSpeed == true then
        if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
            if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").MoveDirection ~= Vector3.new(0,0,0) then
                if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    local to_add = game.Players.LocalPlayer.Character.Humanoid.MoveDirection/15
                    game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame + to_add * ChangeSpeedValue
                end
            end
        end
    end
end)
